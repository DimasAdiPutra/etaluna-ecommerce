{
  "version": 2,
  "build": {
    "env": {
      "NODE_ENV": "development"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Content-Type": "application/json",
        "Cache-Control": "s-max-age=1, stale-while-revalidate"
      },
      "dest": "/"
    }
  ],
  "builds": [
    {
      "src": "./bin/www",
      "use": "@vercel/node"
    }
  ]
}